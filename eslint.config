{
  "parserOptions": { "ecmaVersion": 6 },
  "extends": "airbnb-base",
    rules: {
    'order/properties-alphabetical-order': null,
    'no-shadow': 0,
    'no-param-reassign': 0,
    'eol-last': 0,
  }
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "document": false
  }
}